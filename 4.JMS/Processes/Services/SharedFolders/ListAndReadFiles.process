<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListAndReadFilesRequest" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListAndReadFilesResponse" xmlns:ns02="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/RemoveFilesRequest" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:pfx3="http://www.tibco.com/namespaces/tnt/plugins/file/v2" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns0="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFilesRequest" xmlns:ns03="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/MoveFilesRequest" xmlns:ns2="http://www.tibco.com/schemas/1.Schemas/Resources/Schemas/Commons/Response" xmlns:ns1="http://www.tibco.com/schemas/1.Schemas/Resources/Schemas/Commons/Header">
    <xsd:import namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListAndReadFilesResponse" schemaLocation="/Resources/Schemas/Services/ListAndReadFilesResponse.xsd"/>
    <xsd:import namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/ListAndReadFilesRequest" schemaLocation="/Resources/Schemas/Services/ListAndReadFilesRequest.xsd"/>
    <pd:name>Processes/Services/SharedFolders/ListAndReadFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:ListAndReadFilesRequest"/>
    <pd:startX>88</pd:startX>
    <pd:startY>195</pd:startY>
    <pd:returnBindings>
        <pfx2:ListAndReadFilesResponse>
            <xsl:copy-of select="$Start/pfx:ListAndReadFilesRequest/ns1:Header"/>
            <pfx2:Body>
                <ns2:Response>
                    <ns2:message>
                        <xsl:value-of select="&quot;Files .txt have been extracted&quot;"/>
                    </ns2:message>
                    <ns2:responseStatus>
                        <xsl:value-of select="&quot;SUCCESS&quot;"/>
                    </ns2:responseStatus>
                    <ns2:data>
                        <xsl:copy-of select="$Map-.txt-files/Files"/>
                    </ns2:data>
                </ns2:Response>
            </pfx2:Body>
        </pfx2:ListAndReadFilesResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:ListAndReadFilesResponse"/>
    <pd:endX>994</pd:endX>
    <pd:endY>397</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1770046087128</pd:targetNamespace>
    <pd:activity name="List Files">
        <pd:type>com.tibco.plugin.file.ListFilesActivity</pd:type>
        <pd:resourceType>ae.activities.ListFilesActivity</pd:resourceType>
        <pd:x>248</pd:x>
        <pd:y>221</pd:y>
        <config>
            <mode>files-and-directories</mode>
        </config>
        <pd:inputBindings>
            <ns:ListFilesActivityConfig>
                <fileName>
                    <xsl:value-of select="concat($Start/pfx:ListAndReadFilesRequest/pfx:Body/pfx:source/pfx:sourcePath,$Start/pfx:ListAndReadFilesRequest/pfx:Body/pfx:source/pfx:sourceFilePattern)"/>
                </fileName>
            </ns:ListFilesActivityConfig>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map .txt files">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>283</pd:x>
        <pd:y>357</pd:y>
        <config>
            <element>
                <xsd:element name="Files">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="fileinfo" minOccurs="0" maxOccurs="unbounded">
                                <xsd:complexType>
                                    <xsd:sequence>
                                        <xsd:element name="fullName" type="xsd:string"/>
                                        <xsd:element name="fileName" type="xsd:string"/>
                                    </xsd:sequence>
                                </xsd:complexType>
                            </xsd:element>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <Files>
                <xsl:for-each select="$List-Files/ns:ListFilesActivityOutput/files/fileInfo[contains(fileName, &quot;.txt&quot;) and starts-with(fileName, &quot;file_&quot;)]">
                    <fileinfo>
                        <fullName>
                            <xsl:value-of select="fullName"/>
                        </fullName>
                        <fileName>
                            <xsl:value-of select="fileName"/>
                        </fileName>
                    </fileinfo>
                </xsl:for-each>
            </Files>
        </pd:inputBindings>
    </pd:activity>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>358</pd:x>
        <pd:y>38</pd:y>
        <pd:width>555</pd:width>
        <pd:height>494</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>inputLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:over>$Map-.txt-files/Files/fileinfo</pd:over>
            <pd:iterationElementSlot>currentFile</pd:iterationElementSlot>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:activityOutputName/>
            <pd:accumulateOutput>false</pd:accumulateOutput>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Read File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Do nothing</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>One Bend</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>RemoveFiles</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>One Bend</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>MoveFiles</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>CopyFiles</pd:to>
            <pd:xpathDescription> equals 1</pd:xpathDescription>
            <pd:lineType>Multiple Bends</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Read-File/ns:ReadActivityOutputTextClass/fileContent/textContent = '1'</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>MoveFiles</pd:to>
            <pd:xpathDescription>equals 2</pd:xpathDescription>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>xpath</pd:conditionType>
            <pd:xpath>$Read-File/ns:ReadActivityOutputTextClass/fileContent/textContent = '2'</pd:xpath>
        </pd:transition>
        <pd:transition>
            <pd:from>Read File</pd:from>
            <pd:to>Do nothing</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>otherwise</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>CopyFiles</pd:from>
            <pd:to>RemoveFiles</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Read File">
            <pd:type>com.tibco.plugin.file.FileReadActivity</pd:type>
            <pd:resourceType>ae.activities.FileReadActivity</pd:resourceType>
            <pd:x>475</pd:x>
            <pd:y>291</pd:y>
            <config>
                <encoding>text</encoding>
            </config>
            <pd:inputBindings>
                <pfx3:ReadActivityInputClass>
                    <fileName>
                        <xsl:value-of select="$currentFile/fileinfo/fullName"/>
                    </fileName>
                </pfx3:ReadActivityInputClass>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="CopyFiles">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>614</pd:x>
            <pd:y>107</pd:y>
            <config>
                <processName>/Processes/Services/SharedFolders/CopyFiles.process</processName>
            </config>
            <pd:inputBindings>
                <ns0:CopyFilesRequest>
                    <xsl:copy-of select="$Start/pfx:ListAndReadFilesRequest/ns1:Header"/>
                    <ns0:Body>
                        <ns0:source>
                            <ns0:sourceDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListAndReadFilesRequest/pfx:Body/pfx:source/pfx:sourcePath"/>
                            </ns0:sourceDirectoryPath>
                            <ns0:sourceFileName>
                                <xsl:value-of select="$currentFile/fileinfo/fileName"/>
                            </ns0:sourceFileName>
                        </ns0:source>
                        <ns0:target>
                            <ns0:targetDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListAndReadFilesRequest/pfx:Body/pfx:target/pfx:targetPath"/>
                            </ns0:targetDirectoryPath>
                            <ns0:targetFileName>
                                <xsl:value-of select="concat('final_',$currentFile/fileinfo/fileName)"/>
                            </ns0:targetFileName>
                        </ns0:target>
                    </ns0:Body>
                </ns0:CopyFilesRequest>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="MoveFiles">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>663</pd:x>
            <pd:y>294</pd:y>
            <config>
                <processName>/Processes/Services/SharedFolders/MoveFiles.process</processName>
            </config>
            <pd:inputBindings>
                <ns03:MoveFilesRequest>
                    <xsl:copy-of select="$Start/pfx:ListAndReadFilesRequest/ns1:Header"/>
                    <ns03:Body>
                        <ns03:source>
                            <ns03:sourceDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListAndReadFilesRequest/pfx:Body/pfx:source/pfx:sourcePath"/>
                            </ns03:sourceDirectoryPath>
                            <ns03:sourceFileName>
                                <xsl:value-of select="$currentFile/fileinfo/fileName"/>
                            </ns03:sourceFileName>
                        </ns03:source>
                        <ns03:target>
                            <ns03:targetDirectoryPath>
                                <xsl:value-of select="'C:\Users\TibcoProjects\Files\Input\'"/>
                            </ns03:targetDirectoryPath>
                            <ns03:targetFileName>
                                <xsl:value-of select="concat('Copia_de_',$currentFile/fileinfo/fileName)"/>
                            </ns03:targetFileName>
                        </ns03:target>
                    </ns03:Body>
                </ns03:MoveFilesRequest>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Do nothing">
            <pd:type>com.tibco.plugin.timer.NullActivity</pd:type>
            <pd:resourceType>ae.activities.null</pd:resourceType>
            <pd:x>482</pd:x>
            <pd:y>463</pd:y>
            <config/>
            <pd:inputBindings/>
        </pd:activity>
        <pd:activity name="RemoveFiles">
            <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
            <pd:resourceType>ae.process.subprocess</pd:resourceType>
            <pd:x>734</pd:x>
            <pd:y>105</pd:y>
            <config>
                <processName>/Processes/Services/SharedFolders/RemoveFiles.process</processName>
            </config>
            <pd:inputBindings>
                <ns02:RemoveFilesRequest>
                    <xsl:copy-of select="$Start/pfx:ListAndReadFilesRequest/ns1:Header"/>
                    <ns02:Body>
                        <ns02:source>
                            <ns02:sourceDirectoryPath>
                                <xsl:value-of select="$Start/pfx:ListAndReadFilesRequest/pfx:Body/pfx:source/pfx:sourcePath"/>
                            </ns02:sourceDirectoryPath>
                            <ns02:sourceFileName>
                                <xsl:value-of select="$currentFile/fileinfo/fileName"/>
                            </ns02:sourceFileName>
                        </ns02:source>
                    </ns02:Body>
                </ns02:RemoveFilesRequest>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>List Files</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>List Files</pd:from>
        <pd:to>Map .txt files</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map .txt files</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>