<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFilesRequest" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFilesResponse" xmlns:ns02="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/file" xmlns:ns0="www.tibco.com/plugin/Sleep" xmlns:ns2="http://www.tibco.com/schemas/1.Schemas/Resources/Schemas/Commons/Response" xmlns:ns1="http://www.tibco.com/schemas/1.Schemas/Resources/Schemas/Commons/Header">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFilesRequest" schemaLocation="/Resources/Schemas/Services/CopyFilesRequest.xsd"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFilesResponse"/>
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/3.Files/Resources/Schemas/Services/CopyFilesResponse" schemaLocation="/Resources/Schemas/Services/CopyFilesResponse.xsd"/>
    <pd:name>Processes/Services/SharedFolders/CopyFiles.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx:CopyFilesRequest"/>
    <pd:startX>144</pd:startX>
    <pd:startY>64</pd:startY>
    <pd:returnBindings>
        <pfx2:CopyFilesResponse>
            <xsl:copy-of select="$Start/pfx:CopyFilesRequest/ns1:Header"/>
            <pfx2:Body>
                <ns2:Response>
                    <ns2:message>
                        <xsl:value-of select="'file has been copied'"/>
                    </ns2:message>
                    <ns2:responseStatus>
                        <xsl:value-of select="'SUCCESS'"/>
                    </ns2:responseStatus>
                </ns2:Response>
            </pfx2:Body>
        </pfx2:CopyFilesResponse>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType ref="pfx2:CopyFilesResponse"/>
    <pd:endX>989</pd:endX>
    <pd:endY>87</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1770225690748</pd:targetNamespace>
    <pd:group name="Group">
        <pd:type>com.tibco.pe.core.LoopGroup</pd:type>
        <pd:resourceType>ae.process.group</pd:resourceType>
        <pd:x>285</pd:x>
        <pd:y>20</pd:y>
        <pd:width>387</pd:width>
        <pd:height>316</pd:height>
        <pd:collapsedWidth>57</pd:collapsedWidth>
        <pd:collapsedHeight>70</pd:collapsedHeight>
        <pd:groupVariables/>
        <pd:joinToggle>inherit</pd:joinToggle>
        <config>
            <pd:groupType>errorLoop</pd:groupType>
            <pd:serializable>false</pd:serializable>
            <pd:indexSlot>index</pd:indexSlot>
            <pd:errorCondition>$index = 3</pd:errorCondition>
            <pd:suspendAfterErrorRetry>false</pd:suspendAfterErrorRetry>
        </config>
        <pd:inputBindings/>
        <pd:expanded>true</pd:expanded>
        <pd:transition>
            <pd:from>start</pd:from>
            <pd:to>Copy File</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Copy File</pd:from>
            <pd:to>end</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-16777216</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Copy File</pd:from>
            <pd:to>Sleep</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-3394816</pd:lineColor>
            <pd:conditionType>error</pd:conditionType>
        </pd:transition>
        <pd:transition>
            <pd:from>Sleep</pd:from>
            <pd:to>Generate Error</pd:to>
            <pd:lineType>Default</pd:lineType>
            <pd:lineColor>-6737152</pd:lineColor>
            <pd:conditionType>always</pd:conditionType>
        </pd:transition>
        <pd:activity name="Copy File">
            <pd:type>com.tibco.plugin.file.FileCopyActivity</pd:type>
            <pd:resourceType>ae.activities.FileCopyActivity</pd:resourceType>
            <pd:x>342</pd:x>
            <pd:y>84</pd:y>
            <config>
                <overwrite>true</overwrite>
                <createMissingDirectories>true</createMissingDirectories>
            </config>
            <pd:inputBindings>
                <ns:CopyActivityConfig>
                    <fromFileName>
                        <xsl:value-of select="concat($Start/pfx:CopyFilesRequest/pfx:Body/pfx:source/pfx:sourceDirectoryPath,$Start/pfx:CopyFilesRequest/pfx:Body/pfx:source/pfx:sourceFileName)"/>
                    </fromFileName>
                    <toFileName>
                        <xsl:value-of select="concat($Start/pfx:CopyFilesRequest/pfx:Body/pfx:target/pfx:targetDirectoryPath,$Start/pfx:CopyFilesRequest/pfx:Body/pfx:target/pfx:targetFileName)"/>
                    </toFileName>
                </ns:CopyActivityConfig>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Sleep">
            <pd:type>com.tibco.plugin.timer.SleepActivity</pd:type>
            <pd:resourceType>ae.activities.sleep</pd:resourceType>
            <pd:x>368</pd:x>
            <pd:y>226</pd:y>
            <config/>
            <pd:inputBindings>
                <ns0:SleepInputSchema>
                    <IntervalInMillisec>
                        <xsl:value-of select="$index * 10000"/>
                    </IntervalInMillisec>
                </ns0:SleepInputSchema>
            </pd:inputBindings>
        </pd:activity>
        <pd:activity name="Generate Error">
            <pd:type>com.tibco.pe.core.GenerateErrorActivity</pd:type>
            <pd:resourceType>ae.activities.throw</pd:resourceType>
            <pd:x>533</pd:x>
            <pd:y>215</pd:y>
            <config>
                <faultName/>
            </config>
            <pd:inputBindings>
                <ns02:ActivityInput>
                    <message>
                        <xsl:value-of select="concat('Unterminated literal: Retry attemp',$index)"/>
                    </message>
                </ns02:ActivityInput>
            </pd:inputBindings>
        </pd:activity>
    </pd:group>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Group</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Group</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>